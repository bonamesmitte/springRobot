package com.alice.ui;

import com.alice.entity.Robot;

public class DoRobotConsole implements RobotConsole {

	private Robot robot;
	
	
	
	public DoRobotConsole(Robot robot) {
		this.robot = robot;
	}

	@Override
	public void print() {
		robot.moveForward(),
		robot.moveBackward(), 
		robot.turnRight(), 
		robot.turnLeft(), 
		robot.attack()

	}


