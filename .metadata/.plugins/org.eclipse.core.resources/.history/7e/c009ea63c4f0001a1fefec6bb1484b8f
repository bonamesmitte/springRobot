public class DoRobotConsole implements RobotConsole {

	private Robot robot;
	
	
	
	public DoRobotConsole(Robot robot) {
		this.robot = robot;
	}

	@Override
	public void print() {
		System.out.println(robot.moveForward());

	}

	@Override
	public void setRobot(Robot robot) {
		// TODO Auto-generated method stub
		
	}


